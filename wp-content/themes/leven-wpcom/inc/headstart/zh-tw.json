iv class=\"wp-block-column\"><!-- wp:separator {\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator is-style-wide\"  />\n<!-- /wp:separator -->\n\n<!-- wp:paragraph {\"textColor\":\"foreground-light\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-color has-small-font-size has-foreground-light-color\"><strong>Beverly Mattingdale</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"textColor\":\"foreground-dark\",\"className\":\"has-huge-font-size\"} -->\n<p class=\"has-text-color has-foreground-dark-color has-huge-font-size\">「<em>音訊品質明顯比我工作室其他所有產品更優秀！」</em></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:separator {\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator is-style-wide\"  />\n<!-- /wp:separator -->\n\n<!-- wp:paragraph {\"textColor\":\"foreground-light\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-color has-small-font-size has-foreground-light-color\"><strong>Jefferson Thorpe</strong></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:paragraph {\"textColor\":\"foreground-dark\",\"className\":\"has-huge-font-size\"} -->\n<p class=\"has-text-color has-foreground-dark-color has-huge-font-size\"><em>「音效設計和一致性瞬間就讓我震撼不已。」</em></p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":32} -->\n<div style=\"height:32px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph -->\n<p></p>\n<!-- /wp:paragraph --></div>\n<!-- /wp:column -->\n\n<!-- wp:column -->\n<div class=\"wp-block-column\"><!-- wp:separator {\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator is-style-wide\"  />\n<!-- /wp:separator -->\n\n<!-- wp:paragraph {\"textColor\":\"foreground-light\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-color has-small-font-size has-foreground-light-color\